{
  "ConnectionStrings": {
    "DefaultConnection": "Server=<mysql_server>;Database=<your-database>;Uid=<mysql_user>;Pwd=<mysql_user_password>;Connection Timeout=60;default command timeout=60;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SendGrid": {
    "ApiUser": "<SendGrid_email>",
    "ApiKey": "<SendGrid_api_key>"
  },
  "Security": {
    "JwtSigningKey": "<your_secret>",
    "Audience": "<your_audience>",
    "Issuer": "<your_issuer>",
    "JwtLifetimeInSeconds": "<your_jwt_duration>" 
  }
}
